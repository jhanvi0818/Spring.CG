<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://www.springframework.org/schema/beans 
 http://www.springframework.org/schema/beans/spring-beans-4.0.xsd"> 
    
   
    
        <bean id="emp1" class="com.cg.labbook1d.Employee4"> 
        <property name="id"> 
            <value>101</value> 
        </property> 
        <property name="name"> 
            <value>Sakshi</value> 
        </property> 
        <property name="salary"> 
            <value>2000</value> 
        </property> 
    </bean> 
  
    <bean id="emp2" class="com.cg.labbook1d.Employee4"> 
        <property name="id"> 
            <value>102</value> 
        </property> 
        <property name="name"> 
            <value>Tanvi</value> 
        </property> 
        <property name="salary"> 
            <value>30000</value> 
        </property> 
    </bean> 
  
    <bean id="emp3" class="com.cg.labbook1d.Employee4"> 
    <constructor-arg value="103" type="int"></constructor-arg> 
    <constructor-arg value="Monu" type="java.lang.String"></constructor-arg> 
    <constructor-arg value="16000" type="double"></constructor-arg> 
    </bean> 
  
    <bean id="emp4" class="com.cg.labbook1d.Employee4"> 
    <constructor-arg value="104" type="int"></constructor-arg> 
    <constructor-arg value="Ramesh" type="java.lang.String"></constructor-arg> 
    <constructor-arg value="10000" type="double"></constructor-arg> 
    </bean> 
    
    <bean id="emp5" class="com.cg.labbook1d.Employee4"> 
    <constructor-arg value="105" type="int"></constructor-arg> 
    <constructor-arg value="Swara" type="java.lang.String"></constructor-arg> 
    <constructor-arg value="35000" type="double"></constructor-arg> 
    </bean> 
  <bean id="dao" class="com.cg.labbook1d.EmployeeDao">
        <constructor-arg name="employeeList">
        <list>
         <ref bean="emp1"></ref>
        <ref bean="emp2"></ref>
        <ref bean="emp3"></ref>
        <ref bean="emp4"></ref>
        <ref bean="emp5"></ref>
         </list>
        </constructor-arg>
    </bean>
    
    <bean id="service" class="com.cg.labbook1d.EmployeeService">
    <constructor-arg>
     <ref bean ="dao"/>
    </constructor-arg>
    </bean>
  
</beans> 