<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://www.springframework.org/schema/beans 
 http://www.springframework.org/schema/beans/spring-beans-4.0.xsd"> 
    
   
    
        
<!-- Setter Injection Employee bean-->
	<bean id="emp" class="com.cg.labbook1.ps2.Employee2" autowire="byName">
		<property name="id">
			<value>12345</value>
		</property>
		<property name="name">
			<value>Harriet</value>
		</property>
		<property name="salary">
			<value>40000</value>
		</property>
		
		
		</bean>
		
	
	
	<!-- Setter Injection SBU bean-->
	<bean id="sbu" class="com.cg.labbook1.ps2.SBU">
		<property name="sbuId">
			<value>1</value>
		</property>
		<property name="sbuName">
			<value>Kiran Rao</value>
		</property>
		<property name="sbuHead">
		<value>Product Engineering Services</value>
		</property>
		</bean>
  
</beans> 

